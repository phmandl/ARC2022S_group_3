<?xml version="1.0"?>
<launch>

	<include
		file="$(find racecar)/launch/teleop.launch">
 	</include>

  <include
    file="$(find particle_filter)/launch/localize.launch">
  </include>
  
  <include file="$(find path_planner)/launch/read_raceline.launch"/>

  <node 
      pkg="mpc_path_follow" 
      name="mpc" 
      type="mpc_path_follow_node" 
      output="screen">  
      
      <remap from="/scan" to="/scan"/>
      <remap from="/odom" to="/vesc/odom"/>
      <remap from="/brake_bool" to="/brake_bool"/>
      <remap from="/brake" to="/vesc/low_level/ackermann_cmd_mux/input/safety"/>
      <remap from="/vesc/joy" to="/joy"/>
      <remap from="/nav" to="/vesc/high_level/ackermann_cmd_mux/input/nav_0"/>
  </node>

</launch>